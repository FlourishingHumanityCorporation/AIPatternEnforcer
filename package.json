    "doc:validate:strict": "node tools/templates/doc-validator.js --strict",
    "validate:docs": "node tools/validation/validate-docs.js",
    "doc:test-templates": "node tools/templates/template-tester.js",
    "doc:list": "ls -la templates/documentation/",
    "doc:templates": "cat templates/documentation/TEMPLATE-INDEX.md",
    "doc:dedupe": "node scripts/dev/docs-deduplication.js --analyze",
    "doc:dedupe:dry-run": "node scripts/dev/docs-deduplication.js --dry-run",
    "doc:dedupe:execute": "node scripts/dev/docs-deduplication.js --execute",
    "doc:single-source": "node scripts/dev/single-source-of-truth.js --update",
    "doc:validate-source": "node scripts/dev/single-source-of-truth.js --validate",
    "postinstall": "prisma generate || true"
