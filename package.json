    "claude:quiz:security": "node tools/onboarding/pattern-quiz.js run securityPatterns",
    "hooks:list": "ls -la tools/hooks/",
    "hooks:test": "echo 'Testing Claude Code hooks...' && node tools/hooks/prevent-improved-files.js '{\"file_path\": \"test_improved.js\"}' && echo 'Hooks working!'",
