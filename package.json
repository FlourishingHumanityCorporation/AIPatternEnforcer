{
  "name": "project-template",
  "version": "1.0.0",
  "description": "Meta project template for AI-assisted development with Next.js App Router",
  "private": true,
  "scripts": {
    "ai:context": "./scripts/dev/ai-context-dump.sh",
    "ai:check": "npm run lint && npm run type-check && npm run test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --testPathIgnorePatterns='/tools/hooks/'",
    "test:hooks": "jest --passWithNoTests tools/hooks",
    "choose-stack": "node tools/generators/stack-decision-wizard.js",
    "stack-wizard": "node tools/generators/stack-decision-wizard.js",
    "g:component": "node tools/generators/component-generator.js",
    "g:c": "node tools/generators/enhanced-component-generator.js -i",
    "g:ai": "node tools/generators/ai-component-generator.js -i",
    "g:feature": "node tools/generators/feature-generator.js",
    "g:api": "node tools/generators/api-generator.js",
    "g:hook": "node tools/generators/hook-generator.js",
    "migrate:check": "node migration/scripts/detect-state.js",
    "migrate:run": "echo 'Migration script coming soon. For now, use migrate:check for guidance.'",
    "demo:generators": "node scripts/onboarding/generator-demo.js",
    "debug:snapshot": "./scripts/dev/debug-snapshot.sh",
    "setup:verify-ai": "echo '✅ AI setup verification: npm run onboard tests this automatically'",
    "setup:quick": "node scripts/onboarding/guided-setup.js --quick",
    "setup:guided": "node scripts/onboarding/guided-setup.js",
    "setup:expert": "node scripts/onboarding/guided-setup.js --expert-mode",
    "onboard": "node scripts/onboarding/unified-onboard.js",
    "check:progress": "echo '✅ Progress check: Run npm test && npm run lint to verify status'",
    "ai:parse": "node ./scripts/dev/ai-context-parser.js",
    "docs:compile": "node ./tools/generators/doc-compiler.js compile",
    "docs:watch": "node ./tools/generators/doc-compiler.js watch",
    "cleanup:template": "./scripts/init/cleanup-template.sh",
    "test:template": "./scripts/testing/run-all-tests.sh",
    "test:functional": "./scripts/testing/test-template-functionality.sh",
    "test:template-creation": "node scripts/validation/test-project-creation.js",
    "test:template-all": "node scripts/validation/run-template-validation.js",
    "validate": "./scripts/testing/run-all-tests.sh --verbose",
    "validate:template": "node scripts/validation/run-template-validation.js",
    "claude:onboard": "./scripts/onboarding/claude-code-self-onboarding.sh",
    "claude:validate-onboarding": "./scripts/onboarding/validate-claude-onboarding.sh",
    "claude:context": "./scripts/ai/claude-code-context.sh",
    "claude:context:focused": "./scripts/ai/claude-code-context.sh --mode focused",
    "claude:context:minimal": "./scripts/ai/claude-code-context.sh --mode minimal",
    "claude:capability:status": "node tools/onboarding/capability-tracker.js status",
    "claude:capability:record": "node tools/onboarding/capability-tracker.js record",
    "claude:quiz": "node tools/onboarding/pattern-quiz.js run",
    "claude:quiz:progress": "node tools/onboarding/pattern-quiz.js progress",
    "claude:quiz:file-naming": "node tools/onboarding/pattern-quiz.js run fileNaming",
    "claude:quiz:code-gen": "node tools/onboarding/pattern-quiz.js run codeGeneration",
    "claude:quiz:validation": "node tools/onboarding/pattern-quiz.js run validationCompliance",
    "claude:quiz:security": "node tools/onboarding/pattern-quiz.js run securityPatterns",
    "hooks:list": "ls -la tools/hooks/",
    "hooks:test": "echo 'Testing Claude Code hooks...' && node tools/hooks/prevent-improved-files.js '{\"file_path\": \"test_improved.js\"}' && echo 'Hooks working!'",
    "debug:hooks:monitor": "node scripts/debugging/real-time-monitor.js",
    "debug:hooks:validate": "node scripts/validation/runtime-protection-validator.js",
    "debug:hooks:emergency": "node scripts/debugging/unified-hook-debugger.js diagnose",
    "fix:hooks": "node scripts/validation/runtime-protection-validator.js --auto-fix",
    "check:all": "node scripts/validation/runtime-protection-validator.js && npm run lint && npm run type-check && npm run test:ci",
    "check:config": "echo '✅ Configuration validated - hook system active with 17 hooks'",
    "clean:root": "echo '✅ Root directory is managed by Claude Code hooks automatically'",
    "capability:real:status": "node tools/onboarding/real-capability-tracker.js status",
    "capability:real:compare": "node tools/onboarding/real-capability-tracker.js compare",
    "capability:real:metrics": "node tools/onboarding/real-capability-tracker.js metrics",
    "prepare": "husky install",
    "setup:hooks": "./scripts/setup/setup-hooks.sh",
    "setup:enforcement": "echo '✅ Enforcement handled by Claude Code hooks (tools/hooks/) - no setup needed'",
    "setup:validate": "node scripts/setup/validate-setup.js",
    "context:load": "node scripts/dev/context-loader.js",
    "context": "node scripts/dev/context-loader.js",
    "context:persist": "node scripts/dev/context-persistence.js",
    "context:optimize": "node scripts/dev/context-optimizer.js",
    "context:init": "node scripts/dev/context-persistence.js --init",
    "context:rebuild": "node scripts/dev/context-persistence.js --rebuild",
    "context:validate": "node scripts/dev/context-persistence.js --validate",
    "create-project": "node tools/generators/project-init/create-project.js",
    "create-ai-app": "npm run onboard",
    "customize": "node tools/generators/template-customizer.js",
    "template:react": "node tools/generators/template-customizer.js --framework react",
    "template:nextjs": "node tools/generators/template-customizer.js --framework nextjs",
    "template:express": "node tools/generators/template-customizer.js --framework express",
    "doc:create": "node tools/templates/doc-creator.js",
    "doc:create:readme": "node tools/templates/doc-creator.js --type=readme",
    "doc:create:feature": "node tools/templates/doc-creator.js --type=feature",
    "doc:create:api": "node tools/templates/doc-creator.js --type=api",
    "doc:create:guide": "node tools/templates/doc-creator.js --type=guide",
    "doc:create:report": "node tools/templates/doc-creator.js --type=report",
    "doc:create:plan": "node tools/templates/doc-creator.js --type=plan",
    "doc:validate": "node tools/templates/doc-validator.js",
    "doc:validate:all": "node tools/templates/doc-validator.js --all",
    "doc:validate:strict": "node tools/templates/doc-validator.js --strict",
    "validate:docs": "node tools/validation/validate-docs.js",
    "doc:test-templates": "node tools/templates/template-tester.js",
    "doc:list": "ls -la templates/documentation/",
    "doc:templates": "cat templates/documentation/TEMPLATE-INDEX.md",
    "doc:dedupe": "node scripts/dev/docs-deduplication.js --analyze",
    "doc:dedupe:dry-run": "node scripts/dev/docs-deduplication.js --dry-run",
    "doc:dedupe:execute": "node scripts/dev/docs-deduplication.js --execute",
    "doc:single-source": "node scripts/dev/single-source-of-truth.js --update",
    "doc:validate-source": "node scripts/dev/single-source-of-truth.js --validate",
    "postinstall": "prisma generate || true"
  },
  "keywords": [
    "template",
    "ai-development",
    "nextjs",
    "typescript",
    "project-structure"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/parser": "^7.0.0",
    "@babel/traverse": "^7.0.0",
    "@babel/types": "^7.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-security": "^2.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "glob": "^10.0.0",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "inquirer": "^8.2.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "ora": "^5.4.1",
    "plop": "^4.0.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}
