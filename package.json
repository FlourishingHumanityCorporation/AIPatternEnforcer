    "metrics:friction": "node tools/metrics/user-feedback-system.js friction",
    "metrics:export": "node tools/metrics/user-feedback-system.js export",
    "metrics:start": "node tools/metrics/user-feedback-system.js start-session",
    "metrics:end": "node tools/metrics/user-feedback-system.js end-session",
    "benchmarks": "node tools/metrics/performance-benchmarks.js",
    "benchmarks:generators": "node tools/metrics/performance-benchmarks.js generators",
    "benchmarks:enforcement": "node tools/metrics/performance-benchmarks.js enforcement",
    "benchmarks:setup": "node tools/metrics/performance-benchmarks.js setup",
    "benchmarks:summary": "node tools/metrics/performance-benchmarks.js summary",
    "analytics:generators": "node tools/metrics/generator-analytics.js report",
    "analytics:export": "node tools/metrics/generator-analytics.js export",
    "analytics:reset": "node tools/metrics/generator-analytics.js reset",
    "compliance:setup": "bash scripts/setup-simple-compliance.sh",
    "create-project": "node tools/generators/project-init/create-project.js",
    "customize": "node tools/generators/template-customizer.js",
    "template:react": "node tools/generators/template-customizer.js --framework react",
    "template:nextjs": "node tools/generators/template-customizer.js --framework nextjs",
    "template:express": "node tools/generators/template-customizer.js --framework express"
  },
  "keywords": [
    "template",
    "ai-development",
    "typescript",
    "project-structure"
