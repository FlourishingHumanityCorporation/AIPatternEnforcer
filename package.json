    "claude:context": "./scripts/ai/claude-code-context.sh",
    "claude:context:focused": "./scripts/ai/claude-code-context.sh --mode focused",
    "claude:context:minimal": "./scripts/ai/claude-code-context.sh --mode minimal",
    "claude:capability:status": "node tools/onboarding/capability-tracker.js status",
    "claude:capability:record": "node tools/onboarding/capability-tracker.js record",
    "claude:quiz": "node tools/onboarding/pattern-quiz.js run",
    "claude:quiz:progress": "node tools/onboarding/pattern-quiz.js progress",
    "claude:quiz:file-naming": "node tools/onboarding/pattern-quiz.js run fileNaming",
    "claude:quiz:code-gen": "node tools/onboarding/pattern-quiz.js run codeGeneration",
    "claude:quiz:validation": "node tools/onboarding/pattern-quiz.js run validationCompliance",
    "claude:quiz:security": "node tools/onboarding/pattern-quiz.js run securityPatterns",
    "check:no-improved-files": "node tools/enforcement/no-improved-files.js",
    "check:imports": "node tools/enforcement/check-imports.js",
    "check:documentation-style": "node tools/enforcement/documentation-style.js",
    "check:root": "node tools/enforcement/root-file-enforcement.js",
    "check:root:quiet": "node tools/enforcement/root-file-enforcement.js --quiet",
    "check:banned-docs": "node tools/enforcement/banned-document-types.js",
    "check:banned-docs:quiet": "node tools/enforcement/banned-document-types.js --quiet",
    "check:logs": "node tools/enforcement/log-enforcer.js check",
    "check:logs:status": "node tools/enforcement/log-enforcer.js status",
    "check:config": "node tools/enforcement/config-enforcer.js check",
    "check:config:quiet": "node tools/enforcement/config-enforcer.js check --quiet",
    "check:all": "npm run check:no-improved-files && npm run check:imports && npm run check:documentation-style && npm run check:root && npm run check:banned-docs && npm run check:logs && npm run check:config && npm run validate:docs && npm run docs:check",
    "fix:docs": "node tools/enforcement/fix-docs.js",
    "fix:docs:dry-run": "node tools/enforcement/fix-docs.js --dry-run",
    "fix:logs": "node tools/enforcement/log-enforcer.js fix",
    "fix:logs:dry-run": "node tools/enforcement/log-enforcer.js fix --dry-run",
    "fix:config": "node tools/enforcement/config-enforcer.js fix",
    "fix:config:dry-run": "node tools/enforcement/config-enforcer.js fix --dry-run",
    "enforcement:config": "node tools/enforcement/enforcement-config.js",
    "enforcement:status": "node tools/enforcement/enforcement-config.js status",
    "enforcement:metrics": "node tools/enforcement/enforcement-config.js metrics",
    "prepare": "husky install",
    "setup:enforcement": "node scripts/setup/create-enforcement-scripts.js",
    "setup:log-enforcer": "node tools/enforcement/log-enforcer.js config",
    "setup:hooks": "./scripts/setup/setup-hooks.sh",
    "setup:validate": "node scripts/setup/validate-setup.js",
    "context:load": "node scripts/dev/context-loader.js",
    "context": "node scripts/dev/context-loader.js",
