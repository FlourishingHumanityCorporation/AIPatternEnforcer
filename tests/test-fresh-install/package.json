{
  "name": "project-template",
  "version": "1.0.0",
  "description": "Meta project template for AI-assisted development",
  "private": true,
  "scripts": {
    "ai:context": "./scripts/dev/ai-context-dump.sh",
    "ai:check": "npm run lint && npm run type-check && npm run test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "test": "echo \"Configure your test runner\"",
    "build": "echo \"Configure your build command (e.g., vite build, next build, tsc)\"",
    "security:check": "./scripts/dev/check-security.sh",
    "choose-stack": "node tools/generators/stack-decision-wizard.js",
    "stack-wizard": "node tools/generators/stack-decision-wizard.js",
    "g:component": "node tools/generators/component-generator.js",
    "g:c": "node tools/generators/enhanced-component-generator.js -i",
    "generate:component:enhanced": "node tools/generators/enhanced-component-generator.js",
    "g:feature": "node tools/generators/feature-generator.js",
    "g:api": "node tools/generators/api-generator.js",
    "g:hook": "node tools/generators/hook-generator.js",
    "generate:component": "node tools/generators/component-generator.js",
    "generate:feature": "node tools/generators/feature-generator.js",
    "generate:api": "node tools/generators/api-generator.js",
    "generate:hook": "node tools/generators/hook-generator.js",
    "demo:generators": "node scripts/onboarding/generator-demo.js",
    "debug:snapshot": "./scripts/dev/debug-snapshot.sh",
    "context:optimize": "./scripts/dev/context-optimizer.sh",
    "setup:verify-ai": "./scripts/verify-ai-setup.sh",
    "setup:quick": "node scripts/onboarding/guided-setup.js --quick",
    "setup:guided": "node scripts/onboarding/guided-setup.js",
    "setup:expert": "node scripts/onboarding/guided-setup.js --expert-mode",
    "check:progress": "./scripts/check-progress.sh",
    "ai:focus": "bash ./scripts/dev/ai-focus.sh",
    "ai:parse": "node ./scripts/dev/ai-context-parser.js",
    "docs:compile": "node ./tools/generators/doc-compiler.js compile",
    "docs:watch": "node ./tools/generators/doc-compiler.js watch",
    "cleanup:template": "./scripts/init/cleanup-template.sh",
    "test:template": "./scripts/testing/run-all-tests.sh",
    "test:functional": "./scripts/testing/test-template-functionality.sh",
    "test:ai": "./scripts/testing/test-ai-integration.sh",
    "test:ux": "./scripts/testing/test-user-experience.sh",
    "test:pilot": "./scripts/testing/pilot-validation.sh",
    "test:enforcement": "node tests/enforcement/test-enforcement-system.js",
    "validate": "./scripts/testing/run-all-tests.sh --verbose",
    "validate:docs": "./scripts/validate-docs.sh --ignore-external",
    "validate:docs:all": "./scripts/validate-docs.sh",
    "validate:docs:fix": "./scripts/validate-docs.sh --fix --ignore-external",
    "check:no-improved-files": "node tools/enforcement/no-improved-files.js",
    "check:imports": "node tools/enforcement/check-imports.js",
    "check:documentation-style": "node tools/enforcement/documentation-style.js",
    "check:root": "node tools/enforcement/root-file-enforcement.js",
    "check:root:quiet": "node tools/enforcement/root-file-enforcement.js --quiet",
    "check:banned-docs": "node tools/enforcement/banned-document-types.js",
    "check:banned-docs:quiet": "node tools/enforcement/banned-document-types.js --quiet",
    "check:all": "npm run check:no-improved-files && npm run check:imports && npm run check:documentation-style && npm run check:root && npm run check:banned-docs && npm run validate:docs",
    "fix:docs": "node tools/enforcement/fix-docs.js",
    "fix:docs:dry-run": "node tools/enforcement/fix-docs.js --dry-run",
    "enforcement:config": "node tools/enforcement/enforcement-config.js",
    "enforcement:status": "node tools/enforcement/enforcement-config.js status",
    "enforcement:metrics": "node tools/enforcement/enforcement-config.js metrics",
    "prepare": "husky install",
    "setup:enforcement": "node scripts/setup/create-enforcement-scripts.js",
    "setup:hooks": "./scripts/setup/setup-hooks.sh",
    "context:load": "node scripts/dev/context-loader.js",
    "context": "node scripts/dev/context-loader.js",
    "claude:validate": "node tools/claude-validation/validate-claude.js",
    "claude:test": "node tools/claude-validation/test-compliance.js",
    "claude:stats": "node tools/claude-validation/validate-claude.js --stats",
    "claude:dashboard": "open tools/claude-validation/dashboard.html",
    "claude:config": "node tools/claude-validation/config-manager.js",
    "claude:config:status": "node tools/claude-validation/config-manager.js status",
    "claude:validate:docs": "bash scripts/validate-claude-docs.sh",
    "claude:analytics": "node tools/claude-validation/analytics-tracker.js summary",
    "claude:analytics:export": "node tools/claude-validation/analytics-tracker.js export",
    "metrics:report": "node tools/metrics/user-feedback-system.js report",
    "metrics:friction": "node tools/metrics/user-feedback-system.js friction",
    "metrics:export": "node tools/metrics/user-feedback-system.js export",
    "metrics:start": "node tools/metrics/user-feedback-system.js start-session",
    "metrics:end": "node tools/metrics/user-feedback-system.js end-session",
    "compliance:setup": "bash scripts/setup-simple-compliance.sh"
  },
  "keywords": [
    "template",
    "ai-development",
    "typescript",
    "project-structure"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-security": "^2.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "inquirer": "^8.2.6",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "ora": "^5.4.1",
    "plop": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
