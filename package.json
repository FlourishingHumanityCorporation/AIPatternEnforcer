    "create-ai-app": "npm run onboard",
    "customize": "node tools/generators/template-customizer.js",
    "template:react": "node tools/generators/template-customizer.js --framework react",
    "template:nextjs": "node tools/generators/template-customizer.js --framework nextjs",
    "template:express": "node tools/generators/template-customizer.js --framework express",
