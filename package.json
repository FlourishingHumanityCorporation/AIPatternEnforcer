    "docs:enforce:strict": "node tools/enforcement/documentation/rule-engine.js enforce --mode=pre-commit",
    "docs:check": "(npm run docs:analyze --quiet || true) && (npm run docs:analyze:diff --quiet || true) && npm run docs:enforce",
    "cleanup:template": "./scripts/init/cleanup-template.sh",
    "test:template": "./scripts/testing/run-all-tests.sh",
    "test:functional": "./scripts/testing/test-template-functionality.sh",
    "test:ai": "./scripts/testing/test-ai-integration.sh",
    "test:ux": "./scripts/testing/test-user-experience.sh",
    "test:pilot": "./scripts/testing/pilot-validation.sh",
    "test:vscode": "./scripts/test-vscode-integration.sh",
    "test:enforcement": "node tests/enforcement/test-enforcement-system.js",
    "test:user-journey": "./scripts/testing/user-journey-validator.sh",
    "test:user-program": "./scripts/testing/user-testing-program.sh",
    "test:validation-suite": "./scripts/testing/run-validation-suite.sh",
