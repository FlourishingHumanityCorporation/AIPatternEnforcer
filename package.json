{
  "name": "project-template",
  "version": "1.0.0",
  "description": "Meta project template for AI-assisted development",
  "private": true,
  "scripts": {
    "ai:context": "./scripts/dev/ai-context-dump.sh",
    "ai:check": "npm run lint && npm run type-check && npm run test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "test": "vitest --config config/testing/vitest.config.ts",
    "test:run": "vitest run --config config/testing/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config config/testing/vitest.config.ts",
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "security:check": "./scripts/dev/check-security.sh",
    "choose-stack": "node tools/generators/stack-decision-wizard.js",
    "stack-wizard": "node tools/generators/stack-decision-wizard.js",
    "g:component": "node tools/generators/component-generator.js",
    "g:c": "node tools/generators/enhanced-component-generator.js -i",
    "generate:component:enhanced": "node tools/generators/enhanced-component-generator.js",
    "g:feature": "node tools/generators/feature-generator.js",
    "g:api": "node tools/generators/api-generator.js",
    "g:hook": "node tools/generators/hook-generator.js",
    "generate:component": "node tools/generators/component-generator.js",
    "generate:feature": "node tools/generators/feature-generator.js",
    "generate:api": "node tools/generators/api-generator.js",
    "generate:hook": "node tools/generators/hook-generator.js",
    "demo:generators": "node scripts/onboarding/generator-demo.js",
    "debug:snapshot": "./scripts/dev/debug-snapshot.sh",
    "context:optimize": "./scripts/dev/context-optimizer.sh",
    "setup:verify-ai": "./scripts/verify-ai-setup.sh",
    "setup:quick": "node scripts/onboarding/guided-setup.js --quick",
    "setup:guided": "node scripts/onboarding/guided-setup.js",
    "setup:expert": "node scripts/onboarding/guided-setup.js --expert-mode",
    "check:progress": "./scripts/check-progress.sh",
    "ai:focus": "bash ./scripts/dev/ai-focus.sh",
    "ai:parse": "node ./scripts/dev/ai-context-parser.js",
    "docs:compile": "node ./tools/generators/doc-compiler.js compile",
    "docs:watch": "node ./tools/generators/doc-compiler.js watch",
    "docs:analyze": "node tools/enforcement/documentation/change-analyzer.js",
    "docs:analyze:diff": "node tools/enforcement/documentation/git-diff-analyzer.js",
    "docs:metrics": "node tools/enforcement/documentation/metrics-tracker.js",
