{
  "name": "ai-pattern-enforcer",
  "version": "1.0.0",
  "description": "Meta-project creating production-ready AI app starters with CLI tool",
  "private": true,
  "bin": {
    "create-ai-app": "./bin/create-ai-app.js"
  },
  "scripts": {
    "_comment": "=== ESSENTIAL DEVELOPMENT ===",
    "dev": "echo 'Use: cd starters/minimal-ai-app && npm run dev'",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "check:all": "npm run lint && npm run type-check && npm run test",

    "_comment2": "=== CLI & PROJECT CREATION ===",
    "create": "node bin/create-ai-app.js",
    "onboard": "node scripts/onboarding/unified-onboard.js",

    "_comment3": "=== CODE GENERATION ===",
    "g:c": "node tools/generators/enhanced-component-generator.js -i",
    "g:api": "node tools/generators/api-generator.js", 
    "g:hook": "node tools/generators/hook-generator.js",

    "_comment4": "=== VALIDATION & TESTING ===",
    "validate": "npm run check:all && node scripts/validation/run-template-validation.js",
    "test:hooks": "jest --passWithNoTests tools/hooks",
    "test:templates": "node scripts/validation/test-project-creation.js",

    "_comment5": "=== MIGRATION ===",
    "migrate:check": "node migration/scripts/detect-state.js",
    "migrate:backup": "node migration/scripts/backup-user-work.js", 
    "migrate:run": "node migration/scripts/migrate-components.js",

    "_comment6": "=== DOCUMENTATION ===",
    "doc:create": "node tools/templates/doc-creator.js",
    "doc:validate": "node tools/templates/doc-validator.js",

    "_comment7": "=== DEBUG & MONITORING ===", 
    "debug:snapshot": "./scripts/dev/debug-snapshot.sh",
    "debug:hooks": "node scripts/validation/runtime-protection-validator.js",
    "context": "node scripts/dev/context-loader.js",

    "_comment8": "=== SETUP & MAINTENANCE ===",
    "setup:hooks": "./scripts/setup/setup-hooks.sh",
    "cleanup": "./scripts/init/cleanup-template.sh",
    "prepare": "husky install",
    "postinstall": "prisma generate || true"
  },
  "keywords": [
    "ai-development",
    "nextjs", 
    "template",
    "cli-tool",
    "typescript",
    "project-starter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/parser": "^7.0.0", 
    "@babel/traverse": "^7.0.0",
    "@babel/types": "^7.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-security": "^2.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "glob": "^10.0.0",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "inquirer": "^8.2.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "ora": "^5.4.1",
    "plop": "^4.0.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts_removed": {
    "note": "Reduced from 130+ scripts to 30 essential scripts",
    "categories_eliminated": [
      "claude:quiz:* (8 scripts) - Consolidated into onboard",
      "claude:capability:* (3 scripts) - Redundant with validation", 
      "debug:hooks:* (4 scripts) - Simplified to debug:hooks",
      "context:* (6 scripts) - Simplified to context",
      "doc:dedupe:* (3 scripts) - Manual maintenance task",
      "capability:real:* (3 scripts) - Development-only tracking",
      "template:* (3 scripts) - Covered by CLI tool",
      "setup:* (5 scripts) - Simplified to essential setup",
      "AI-specific duplicates (20+ scripts) - Consolidated",
      "Status/echo scripts (15+ scripts) - Removed noise"
    ]
  }
}